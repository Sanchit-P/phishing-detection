
{
  "manifest_version": 3,
  "name": "PhishGuard AI",
  "version": "1.1",
  "permissions": ["scripting", "activeTab", "tabs", "storage"],
  "host_permissions": ["http://127.0.0.1:5000/*", "https://mail.google.com/*"],
  "action": { "default_popup": "popup.html" },
  "content_scripts": [{
    "matches": ["https://mail.google.com/*"],
    "js": ["content.js"]
  }]
}